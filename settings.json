{
    "workbench.iconTheme": "material-icon-theme",
    "editor.indentSize": "tabSize",
    "editor.mouseWheelZoom": true,
    "editor.lineHeight": 32,
    "vscode_custom_css.imports": [
        "file:///home/huong/Documents/style.css"
    ],
    "indentRainbow.colors": [
        "rgba(24, 78, 119,0.15)",
        "rgba(30, 96, 145,0.15)",
        "rgba(26, 117, 159, 0.15)",
        "rgba(22, 138, 173,0.15)",
        "rgba(52, 160, 164,0.15)",
        "rgba(82, 182, 154,0.15)",
        "rgba(82, 182, 154,0.1)",
        "rgba(82, 182, 154,0.05)",
        "rgba(82, 182, 154,0.025)"
    ],
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on"
    },
    "editor.letterSpacing": 0.5,
    "powermode.shake.intensity": 0,
    "powermode.enabled": true,
    "files.autoGuessEncoding": true,
    "editor.fontLigatures": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "editor.bracketPairColorization.enabled": true,
    "editor.linkedEditing": true,
    "powermode.combo.location": "statusbar",
    "editor.cursorStyle": "line",
    "editor.tabSize": 4,
    "editor.renderWhitespace": "all",
    "editor.renderLineHighlight": "all",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "expand",
    "github.copilot.editor.enableAutoCompletions": true,
    "gitlens.gitCommands.skipConfirmations": [
        "fetch:command",
        "stash-push:command",
        "switch:command"
    ],
    "editor.unicodeHighlight.nonBasicASCII": false,
    "workbench.editorAssociations": {
        "*.php": "default"
    },
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "files.associations": {
        "*.js": "javascript",
        ".env*": "dotenv"
    },
    "javascript.validate.enable": false,
    "javascript.suggest.autoImports": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "emmet.syntaxProfiles": {
        "javascript": "jsx"
    },
    "files.eol": "\r\n",
    "[javascriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "files.exclude": {
        "**/largeFolder": true
    },
    "editor.fontFamily": "SF Mono, Source Code Pro, Input Mono, Inconsolata, Arial, Helvetica, sans-serif, Consolas, 'Courier New', monospace",
    "workbench.settings.applyToAllProfiles": [
        "editor.fontFamily",
        "editor.detectIndentation"
    ],
    "settingsSync.ignoredSettings": [
        "editor.fontFamily"
    ],
    "workbench.editor.enablePreview": false,
    "laravel-pint.enable": true,
    "workbench.editor.showTabs": "multiple",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 15,
    "editor.renderControlCharacters": true,
    "[typescriptreact]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.guides.indentation": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "security.workspace.trust.untrustedFiles": "open",
    "git.confirmSync": false,
    "git.suggestSmartCommit": false,
    "editor.guides.bracketPairs": "active",
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "editor.autoIndent": "brackets",
    "git.ignoreRebaseWarning": true,
    "gitlens.graph.minimap.enabled": false,
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar"
    },
    "github.copilot.chat.experimental.temporalContext.enabled": true,
    "explorer.confirmPasteNative": false,
    "php.validate.enable": false,
    "php.suggest.basic": false,
    "explorer.confirmDragAndDrop": false,
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#e778af"
    },
    "editor.cursorWidth": 2,
    "workbench.colorTheme": "Evondev Dracula High Contrast",
    "terminal.integrated.fontSize": 15,
    "github.copilot.advanced": {
        "authProvider": "github"
    },
    "editor.minimap.enabled": true,
    "git.autofetch": true
}